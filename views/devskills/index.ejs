<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>Dev Skills</title>
</head>

<body>
    <h1>My Dev Skills</h1>
    <ul>
        <% devSkills.forEach(function(ele) { %>
            <li>
                <a href="/devSkills/<%= ele.id %>">
                    <%= ele.skill %>
                </a>
                <%= ele.skill %>
                    -
                    <%= `Proficiency Level: ${ele.proficiency}` %>
            </li>
            <% }); %>
    </ul>
</body>

</html>